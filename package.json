{
  "name": "immutablets",
  "version": "0.10.0",
  "description": "Immutable state management for TypeScript",
  "author": "Leon Adler <git@leon-adler.com>",
  "license": "MIT",
  "keywords": [
    "immutable",
    "mutable",
    "redux",
    "flux",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leonadler/immutablets.git"
  },
  "bugs": {
    "url": "https://github.com/leonadler/immutablets/issues"
  },
  "homepage": "https://github.com/leonadler/immutablets#readme",
  "main": "es5/index.js",
  "esnext:main": "es6/index.js",
  "e2015": "es6/index.js",
  "typings": "declarations/index.d.ts",
  "scripts": {
    "build": "gulp build",
    "build-tsc": "tsc -p tsconfig.es5.json && tsc -p tsconfig.es6.json",
    "prepublishOnly": "gulp default",
    "test": "mocha --require ts-node/register --require source-map-support/register src/**/*.spec.ts",
    "watch-tests": "mocha --watch --reporter min --require ts-node/register --require source-map-support/register src/**/*.spec.ts",
    "watch": "gulp watch"
  },
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/del": "^2.2.32",
    "@types/gulp-mocha": "0.0.30",
    "@types/gulp-sourcemaps": "0.0.31",
    "@types/gulp-util": "^3.0.31",
    "@types/merge2": "^0.3.30",
    "@types/mocha": "^2.2.41",
    "@types/orchestrator": "0.3.0",
    "@types/source-map-support": "^0.4.0",
    "@types/vinyl": "^2.0.0",
    "chai": "^4.0.2",
    "del": "^3.0.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-mocha": "~3.0.1",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-typescript": "^3.1.7",
    "merge2": "^1.0.3",
    "mocha": "^3.4.2",
    "reflect-metadata": "^0.1.10",
    "source-map-support": "^0.4.15",
    "ts-node": "^3.1.0",
    "typescript": "^2.4.1"
  }
}
