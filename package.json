{
  "name": "immutablets",
  "version": "0.12.0",
  "description": "Immutable state management for TypeScript",
  "author": "Leon Adler <git@leon-adler.com>",
  "license": "MIT",
  "keywords": [
    "immutable",
    "mutable",
    "redux",
    "flux",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leonadler/immutablets.git"
  },
  "bugs": {
    "url": "https://github.com/leonadler/immutablets/issues"
  },
  "homepage": "https://github.com/leonadler/immutablets#readme",
  "main": "es5/index.js",
  "esnext:main": "es6/index.js",
  "e2015": "es6/index.js",
  "typings": "declarations/index.d.ts",
  "scripts": {
    "build": "gulp build",
    "build-tsc": "tsc -p tsconfig.es5.json && tsc -p tsconfig.es6.json",
    "prepublishOnly": "gulp default",
    "test": "mocha --require ts-node/register --require source-map-support/register src/**/*.spec.ts",
    "watch-tests": "mocha --watch --reporter min --require ts-node/register --require source-map-support/register src/**/*.spec.ts",
    "watch": "gulp watch"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/del": "^3.0.1",
    "@types/gulp": "^4.0.5",
    "@types/gulp-sourcemaps": "0.0.32",
    "@types/merge2": "^1.1.4",
    "@types/mocha": "^5.2.5",
    "@types/orchestrator": "0.3.2",
    "@types/source-map-support": "^0.4.1",
    "@types/vinyl": "^2.0.2",
    "chai": "^4.2.0",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^5.0.0",
    "merge2": "^1.2.3",
    "mocha": "^5.2.0",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.10",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2"
  }
}
